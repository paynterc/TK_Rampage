var enemy2 = instance_create(global.bound_r - xs,0,ob);

enemy1.waypoints = waypoints_1;
enemy2.waypoints = waypoints_2;

e_count--;

if(e_count&lt;1){
    waypoints_1=noone;
    waypoints_2=noone;
